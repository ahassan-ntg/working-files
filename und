    column_exists   NUMBER := 0;

    CURSOR c1 IS
        (SELECT table_name FROM all_tables WHERE LOWER(table_name) LIKE 'grid_att_%');
BEGIN
    FOR i IN c1
    LOOP
        SELECT COUNT (table_name)
           INTO column_exists
           FROM user_tab_cols tc
          WHERE LOWER (tc.table_name) = LOWER (i)
                AND LOWER (tc.column_name) = LOWER ('attachment_note');

        IF (column_exists < 1)
        THEN
            EXECUTE IMMEDIATE   'ALTER TABLE '
                             || i
                             || ' ADD attachment_note VARCHAR2(255)';
        END IF;
    END LOOP;
END;
--end;


