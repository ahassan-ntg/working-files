scheduleMeeting(field){

        let isMeetingSchedulerRequestBodyValid = true;
        let meetingSchedulerRequestBody = this.msGraphService.configureMeetingSchedulerRequestBody(field);

        isMeetingSchedulerRequestBodyValid = meetingSchedulerRequestBody != null
            &&  meetingSchedulerRequestBody != undefined;

        if(isMeetingSchedulerRequestBodyValid && this.msalGuardConfig.authRequest)
        {
            // this.ShowSaveButtonSpin=true;
            // this.msalService.loginPopup({...this.msalGuardConfig.authRequest} as RedirectRequest).subscribe(response=>{
                    // this.ShowSaveButtonSpin=true;

                    this.msGraphService.createEvent(meetingSchedulerRequestBody).subscribe((response:any)=>{
                        // this.ShowSaveButtonSpin=false;
                        meetingSchedulerRequestBody.id = response.id;
                        field.meetingId = response.id;
                        this._serviceCall.putObservable(field,this._serviceUrls.putMeetingIdURL,false).subscribe(
                            response =>
                                this._serviceCall.alert("Meeting Scheduled Successfully!",Constant.ALERT_TYPE_SUCCESS)
                        );

                    }, (error)=>{
                        this._serviceCall.alert(error.message);
                    });
            //     }
            // )
        }
    }
    for (let i = 0; i < this.genericobject.udasValues.length; i++) {
                    if (this.genericobject.udasValues[i].udaType == IUDAType.MEETING_SCHEDULER) {
                        this.scheduleMeeting(this.genericobject.udasValues[i], this.genericobject.recId,false)
                    }

                    if (this.genericobject.udasValues[i].udaType == IUDAType.GRID) {
                        let tempGridObject = this.genericobject.udasValues[i];
                        this.genericobject.udasValues[i].fields.forEach(field => {
                            if (field.type == IUDAType.MEETING_SCHEDULER) {
                                this.genericobject.udasValues[i].rowData.forEach(row => {

                                    this.genericobject.udasValues[i].meetingTitle = row[field.name + "_meeting_title"];
                                    this.genericobject.udasValues[i].meetingStartDateTime = row[field.name + "_meeting_start_date_time"];
                                    this.genericobject.udasValues[i].meetingEndDateTime = row[field.name + "_meeting_end_date_time"];
                                    this.genericobject.udasValues[i].meetingAttendees = row[field.name + "_meeting_attendees"];
                                    this.genericobject.udasValues[i].meetingAttendeesArray = row[field.name + "_meeting_attendees"].split(',');

                                    debugger;

                                        this.scheduleMeeting(this.genericobject.udasValues[i], row['recid'], true, field.name);
                                    this.genericobject.udasValues[i].meetingTitle = "";
                                    this.genericobject.udasValues[i].meetingStartDateTime = "";
                                    this.genericobject.udasValues[i].meetingEndDateTime = "";
                                    this.genericobject.udasValues[i].meetingAttendees = "";
                                    this.genericobject.udasValues[i].meetingAttendeesArray = [];

                                })

                            }
                        });
                    }
                }

                        debugger;

